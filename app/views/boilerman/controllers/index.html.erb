<div class="row">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">Filter</h3>
    </div>
    <div class="panel-body">

      <%= render "controller_filter" %>
      <%= render "action_filter" %>

      <button onclick="clearLocalFilters()" class="btn btn-warning pull-right">Clear Filters</button>
    </div>
  </div>

</div>

<div class="row">
  <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    <%= render "controller_list_panel" %>
    <%= render "callback_breakdown_panel" %>
    <%= render "application_statistics_panel" %>
  </div>
</div>

